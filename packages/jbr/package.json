{
  "name": "jbr",
  "version": "6.0.0",
  "description": "Just a Benchmark Runner",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubensworks/jbr.js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubensworks/jbr.js.git",
    "directory": "packages/jbr"
  },
  "bugs": {
    "url": "https://github.com/rubensworks/jbr.js/issues"
  },
  "keywords": [
    "jbr",
    "just a benchmark runner",
    "benchmarking",
    "command line"
  ],
  "main": "lib/index.js",
  "typings": "lib/index",
  "lsd:module": true,
  "publishConfig": {
    "access": "public"
  },
  "bin": "bin/jbr",
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "bin/jbr",
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.json",
    "lib/templates",
    "lib/templates/.gitignore"
  ],
  "engines": {
    "node": ">=18.0"
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls"
  },
  "dependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/cross-spawn": "^6.0.0",
    "@types/dockerode": "^3.3.0",
    "@types/fs-extra": "^11.0.0",
    "@types/pidusage": "^2.0.0",
    "@types/semver": "^7.7.0",
    "@types/tar": "^6.1.0",
    "@types/yargs": "^17.0.0",
    "cli-progress": "^3.12.0",
    "componentsjs": "^6.0.0",
    "cross-fetch": "^4.0.0",
    "cross-spawn": "^7.0.0",
    "dockerode": "^4.0.0",
    "fs-extra": "^11.0.0",
    "ora": "^8.2.0",
    "pidusage": "^4.0.0",
    "semver": "^7.7.0",
    "tar": "^7.4.0",
    "winston": "^3.17.0",
    "yargs": "^17.0.0"
  }
}
