{
  "name": "@jbr-hook/cli",
  "version": "6.0.0",
  "description": "CLI-based hook handler for JBR",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubensworks/jbr.js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubensworks/jbr.js.git",
    "directory": "packages/hook-cli"
  },
  "bugs": {
    "url": "https://github.com/rubensworks/jbr.js/issues"
  },
  "keywords": [
    "hook",
    "CLI",
    "jbr",
    "benchmarking"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "typings": "lib/index",
  "lsd:module": true,
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/templates"
  ],
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls"
  },
  "peerDependencies": {
    "jbr": ">=6.0.0"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.0",
    "fs-extra": "^11.0.0"
  },
  "devDependencies": {
    "jbr": "^6.0.0"
  }
}
